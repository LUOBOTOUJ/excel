<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vitality.material.mapper.PoMapper">
    <insert id="insertReturnId" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
    insert into tbl_imp_po(prepareReceiptCode,offeredDate,supplierCode,supplierName,factoryInvoice,deleteStatus,create_date)
      VALUES(#{prepareReceiptCode},#{offeredDate},#{supplierCode},#{supplierName},#{factoryInvoice},#{deleteStatus},#{createDate})
    </insert>

</mapper>
